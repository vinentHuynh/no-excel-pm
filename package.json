{
  "name": "paroview",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel --filter \"./packages/**\" dev",
    "deploy:backend": "node scripts/deploy-all.js --backend-only",
    "deploy:frontend": "node scripts/deploy-all.js --frontend-only",
    "deploy:cloudfront": "pnpm --filter \"./packages/backend\" cdk deploy ParoviewCloudFrontStack --outputs-file outputs.json",
    "deploy:all": "node scripts/deploy-all.js"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.637.0",
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/client-sts": "^3.637.0",
    "@aws-sdk/client-iam": "^3.637.0"
  },
  "devDependencies": {
    "typescript": "~5.9.3"
  }
}
